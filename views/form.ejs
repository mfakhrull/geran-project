<!-- Page Content -->
<div id="page-content-wrapper">
  <button type="button" class="hamburger animated fadeInLeft is-closed" data-toggle="offcanvas">
    <span class="hamb-top"></span>
    <span class="hamb-middle"></span>
    <span class="hamb-bottom"></span>
  </button>
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-lg-offset-2">

        <div class="bx-logo-form">
          <img class="logo-form" src="/images/mada-logo.png" alt="logo-mada">
        </div>

        <form class="text-start" action="/pendaftaran" method="post">
          <div class="row mb-5">
            <label for="inputtext3" class="fw-bold col-sm-4 col-form-label">Bahagian A: MAKLUMAT PEMOHON</label>
          </div>
          <div class="row mb-3">
            <label for="inputtext3" class="col-sm-2 col-form-label">Nama</label>
            <div class="col-sm-5">
              <input name="nama" type="text" class="form-control" id="inputtext3"  value="<%= nama %>">
            </div>
          </div>
          <div class="row mb-3">
            <label for="text" class="col-sm-2 col-form-label">Nombor MyKad</label>
            <div class="col-sm-5">
              <input name="mykad" type="text" class="form-control" id="inputtext3" value="<%= mykad %>">
            </div>
          </div>
          <div class="row mb-3">
            <label for="inputtext3" class="col-sm-2 col-form-label">Jantina</label>
            <div class="col-sm-5">
              <input name="jantina" type="text" class="form-control" id="inputEmail3" value="<%= jantina %>">
            </div>
          </div>
          <div class="row mb-3">
            <label for="inputtext3" class="col-sm-2 col-form-label">Nombor Telefon Bimbit</label>
            <div class="col-sm-5">
              <input name="tel" type="text" class="form-control" id="inputtext3" value="<%= tel %>">
            </div>
          </div>
          <div class="row mb-3">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Email</label>
            <div class="col-sm-5">
              <input name="emailPendaftaran" type="text" class="form-control" id="inputEmail3" value="<%= emailPendaftaran %>">
            </div>
          </div>
          <div class="row mb-3">
            <label for="inputtext3" class="col-sm-2 col-form-label">Nama Akaun Bank</label>
            <div class="col-sm-5">
              <input name="namaAkaun" type="text" class="form-control" id="inputtext3" value="<%= namaAkaun %>">
            </div>
          </div>
          <div class="row mb-3">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Nombor Akaun Bank Persendirian Pemohon</label>
            <div class="col-sm-5">
              <input name="noAkaun" type="text" class="form-control" id="inputEmail3" value="<%= noAkaun %>">
            </div>
          </div>
          <div class="row mb-3">
            <label for="inputtext3" class="col-sm-2 col-form-label">Status Perkahwinan</label>
            <div class="col-sm-5">
              <input name="statusPerkahwinan" type="text" class="form-control" id="inputtext3" value="<%= statusPerkahwinan %>">
            </div>
          </div>
          <div class="row mb-3">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Alamat Surat-Menyurat</label>
            <div class="col-sm-5">
              <input name="alamat" type="text" class="form-control" id="inputEmail3" value="<%= alamat %>">
            </div>
          </div>
          <div class="row mb-5">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Tarikh Daftar</label>
            <div class="col-sm-5">
              <input id="datePicker" name="tarikhDaftar" type="date" class="form-control" value="<%= tarikhDaftar %>">
            </div>
          </div>
          <div class="row mb-5">
            <label for="inputtext3" class="fw-bold col-sm-4 col-form-label">Bahagian B: MAKLUMAT PASANGAN</label>
          </div>
          <div class="row mb-3">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Nama Pasangan</label>
            <div class="col-sm-5">
              <input name="namaPasangan" type="text" class="form-control" id="inputEmail3" value="<%= namaPasangan %>">
            </div>
          </div>
          <div class="row mb-3">
            <label for="inputtext3" class="col-sm-2 col-form-label">Jenis Pengenalan</label>
            <div class="col-sm-5">
              <input name="jenisPengenalan" type="text" class="form-control" id="inputtext3" value="<%= jenisPengenalan %>">
            </div>
          </div>
          <div class="row mb-3">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Nombor Pengenalan</label>
            <div class="col-sm-5">
              <input name="nomborPengenalan" type="text" class="form-control" id="inputEmail3" value="<%= nomborPengenalan %>">
            </div>
          </div>

          <div class="mt-5">
            <button class="btn btn-secondary" type="submit" name="form-submit">Submit</button>
          </div>


        </form>


      </div>
    </div>
  </div>
</div>
<!-- /#page-content-wrapper -->


<!-- /#wrapper -->

